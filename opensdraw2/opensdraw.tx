
LegoModel:
    model_elements*=ModelElements
;

ModelElements:
    Import | Function | Part | Block
;

Import:
    "(" "import"  name = ID ")"
;

Function:
    "(" "def" ID
        parts*=Block
     ")"
;

Part:
    "(" "tb" name = STRING ")"
;

Block:
    "(" "block"
        parts*=Part
     ")"
;

Comment:
  /\/\/.*$/
;